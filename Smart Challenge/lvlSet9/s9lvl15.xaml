            	<EasingDoubleKeyFrame KeyTime="0:0:5.2" Value="226.866"/>
            	<EasingDoubleKeyFrame KeyTime="0:0:5.8" Value="486.567"/>
            	<EasingDoubleKeyFrame KeyTime="0:0:6.5" Value="335.821"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Name="go3">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateX)" Storyboard.TargetName="u3">
                <EasingDoubleKeyFrame KeyTime="0:0:7.2" Value="2.985"/>
                <EasingDoubleKeyFrame KeyTime="0:0:7.8" Value="410.448"/>
                <EasingDoubleKeyFrame KeyTime="0:0:8.3" Value="846.269"/>
                <EasingDoubleKeyFrame KeyTime="0:0:8.8" Value="697.015"/>
                <EasingDoubleKeyFrame KeyTime="0:0:9.3" Value="568.657"/>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(CompositeTransform.TranslateY)" Storyboard.TargetName="u3">
                <EasingDoubleKeyFrame KeyTime="0:0:7.2" Value="-4.478"/>
                <EasingDoubleKeyFrame KeyTime="0:0:7.8" Value="-334.329"/>
                <EasingDoubleKeyFrame KeyTime="0:0:8.3" Value="-140.299"/>
                <EasingDoubleKeyFrame KeyTime="0:0:8.8" Value="14.925"/>
                <EasingDoubleKeyFrame KeyTime="0:0:9.3" Value="-94.03"/>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
    </phone:PhoneApplicationPage.Resources>

