            </Image.RenderTransform>
        </Image>
        <Image x:Name="u2" HorizontalAlignment="Left" Height="70" Margin="396,-99,0,0" VerticalAlignment="Top" Width="138" Source="/Images/imgLvl9/ufo.png" RenderTransformOrigin="0.5,0.5" Tap="u2_Tap">
            <Image.RenderTransform>
